class Solution{
public:	
	// Function returns the second
	// largest elements

	int print2largest(int arr[], int n) {
	    int a=0;
	    int b=-1;
	    
     for(int i=0;i<n;i++){
         if(arr[i]>a){
             a=arr[i];
         }
        
         
     }	
      for(int i=0;i<n;i++){
         if(arr[i]>b && arr[i]!=a){
             b=arr[i];
         }
        
         
     }
