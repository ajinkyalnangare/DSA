class Solution {
  public:
    int stockBuyAndSell(int n, vector<int> &prices) {
        int profit=0;
        for(int i=0;i<n-1;i++){
            if(prices[i]<prices[i+1]){
                profit=profit+(prices[i+1]-prices[i]);
               
            }
        }
         
        return profit;
    }
};
